<h1>Atendentes</h1>

<table class="table table-condensed">
  <thead>
    <tr>
      <th>Name</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @atendentes.each do |atendente| %>
      <tr>
        <td><%= atendente.name %></td>
        <td><%= link_to 'Mostrar', atendente %></td>
        <td><%= link_to 'Editar', edit_atendente_path(atendente) %></td>
        <td><%= link_to 'Deletar', atendente, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'Cadastrar Novo Atendente', new_atendente_path %>
